<html>
	<head>
		<title>Venus As a Boy</title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">		
		<link href="css/style.css" type="text/css" rel="stylesheet" />
		<link href="css/about.css" rel="stylesheet" />
		<link href="css/mural.css" rel="stylesheet" />
		<link href="css/engines.css" rel="stylesheet" />
	</head>
	<body>
		<div class="container-fluid">				
			<div class="row menu">
				<div class="col-xs-4 col-md-1">
					<% if menu_item == "home" %>
						<a class="highlight" href="/">home</a>
					<% else %>
						<a href="/">home</a>
					<% end %>
				</div>
				<div class="col-xs-4 col-md-1">
					<% if menu_item == "murals" %>
						<a class="highlight" href="murals">murals</a>
					<% else %>
						<a href="murals">murals</a>
					<% end %>
				</div>		
				<div class="col-xs-4 col-md-1">
					<% if menu_item == "engines" %>
						<a class="highlight" href="engines">engines</a>
					<% else %>
						<a href="engines">engines</a>
					<% end %>
				</div>		
				<div class="col-xs-4 col-md-1">
					<% if menu_item == "about" %>
						<a class="highlight" href="about">about</a>
					<% else %>
						<a href="about">about</a>
					<% end %>
				</div>								
			</div>
			<div class="row">
				<div class="content vertical-align">
					<%= yield %>
				</div>
			</div>		
		</div>
	</body>
</html>